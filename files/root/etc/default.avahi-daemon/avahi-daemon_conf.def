CAPTION='Avahi-daemon: Main config file'
DESCRIPTION='<a href="http://avahi.org/download/avahi-daemon.conf.5.xml" target="_blank">Syntax see documentation of avahi-daemon_conf</a><br>'

CONFIG_FILE='/tmp/flash/avahi-daemon/avahi-daemon_conf'
CONFIG_SAVE='modsave flash; if pidof avahi-daemon > /dev/null; then echo ""; /mod/etc/init.d/rc.avahi-daemon restart; fi'
CONFIG_TYPE='text'

TEXT_ROWS=20
