CAPTION='Avahi-daemon: Static hosts file'
DESCRIPTION='<a href="http://avahi.org/download/avahi.hosts.5.xml" target="_blank">Syntax see documentation of avahi_hosts</a><br>'

CONFIG_FILE='/tmp/flash/avahi-daemon/avahi_hosts'
CONFIG_SAVE='modsave flash; if pidof avahi-daemon > /dev/null; then echo ""; /mod/etc/init.d/rc.avahi-daemon restart; fi'
CONFIG_TYPE='text'

TEXT_ROWS=20
